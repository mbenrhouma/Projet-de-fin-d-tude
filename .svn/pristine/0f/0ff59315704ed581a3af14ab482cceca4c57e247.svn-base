								<f:facet name="header">
									<h:outputText value="Date début" />
								</f:facet>
								<h:outputText value="#{current.dateFin}">
									<f:convertDateTime pattern="dd/MM/yyyy"/>
								</h:outputText>
					</p:column>
					
