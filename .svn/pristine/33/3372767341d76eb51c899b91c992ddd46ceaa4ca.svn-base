			<resource-bundle>
			<base-name>com.cynapsys.bundles.parametrage-resources_fr</base-name>
			<var>paramfrmsgs</var>
		</resource-bundle>	
			<resource-bundle>
			<base-name>com.cynapsys.bundles.parametrage-resources_ar</base-name>
			<var>paramarmsgs</var>
			</resource-bundle>
