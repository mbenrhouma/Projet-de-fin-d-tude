        							<p:selectBooleanCheckbox value="#{rechercheDesClientsAcquereursBean.cessionSalaire}" >
        							<p:ajax update="CActive CEchue"></p:ajax>
        							</p:selectBooleanCheckbox>
        							<p:spacer width="15px" />
        							<h:outputText value="Cession sur salaire" />
        							<p:spacer width="15px" />
        							
        							<p:selectBooleanCheckbox id="CActive" value="#{rechercheDesClientsAcquereursBean.cessionActive}" disabled="#{rechercheDesClientsAcquereursBean.cessionSalaire!=true}"/>
        							<p:spacer width="15px" />
        							<h:outputText value="Cession Active" />
        							<p:spacer width="15px" />
        							
        							<p:selectBooleanCheckbox id="CEchue" value="#{rechercheDesClientsAcquereursBean.cessionEchue}" disabled="#{rechercheDesClientsAcquereursBean.cessionSalaire!=true}"/>
        							<p:spacer width="15px" />
        							<h:outputText value="Cession échue" />
								</center>
							</p:column>
							<p:column>
								<h:outputText value="" styleClass="required-star-right" />
							</p:column>
							<p:column style="text-align: right" width="150">
								<h:outputText value="#{precarmsgs['clientAcqueruer.Cession']}:" dir="rtl" styleClass="weightBoldAr" />
							</p:column>
						</p:row>
						




