										<f:selectItem itemLabel="Echu" itemValue="n" />
										<f:selectItem itemLabel="Tous" itemValue="t" />
									</p:selectOneRadio>
								</center>
							</p:column>
							<p:column>
								<h:outputText value="" styleClass="required-star-right" />
							</p:column>
							<p:column style="text-align: right" width="150">
								<h:outputText value="#{precarmsgs['clientAcqueruer.etatAmortissement']}:" dir="rtl" styleClass="weightBoldAr" />
							</p:column>
						</p:row>
						
						
						
						<!-- 					Champs recherche par : cession -->
