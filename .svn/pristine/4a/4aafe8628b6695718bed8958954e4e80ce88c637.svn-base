										<f:selectItem itemLabel="Non" itemValue="false" />
										<f:selectItem itemLabel="Tous" itemValue="" />
									</p:selectOneRadio>
								</center>
							</p:column>
							<p:column>
								<h:outputText value="" styleClass="required-star-right" />
							</p:column>
							<p:column style="text-align: right" width="150">
								<h:outputText value="#{precarmsgs['clientAcqueruer.enArrangement']}:" dir="rtl" styleClass="weightBoldAr" />
							</p:column>
						</p:row>
						
						<!-- 					Champs recherche par : etat amortissement -->
