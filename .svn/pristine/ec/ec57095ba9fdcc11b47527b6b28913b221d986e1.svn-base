package com.cynapsys.Views;

import java.io.Serializable;

import javax.annotation.PostConstruct;
import javax.faces.bean.ManagedBean;
import javax.faces.bean.ViewScoped;


@ManagedBean(name = "acceuilBean")
@ViewScoped
public class AcceuilBean implements Serializable {

	private static final long serialVersionUID = 1L;



	private Integer nbNotifAugmentationMontantContrat;
	private Integer nbNewNotifAugmentationMontantContrat;
	private Integer nbAvisImprimer;
	private Integer nbRelance;


	@PostConstruct
	public void init() {

		
		
		
	}


	public Integer getNbNotifAugmentationMontantContrat() {
	
		return nbNotifAugmentationMontantContrat;
	}

	public void setNbNotifAugmentationMontantContrat(
			Integer nbNotifAugmentationMontantContrat) {
		this.nbNotifAugmentationMontantContrat = nbNotifAugmentationMontantContrat;
	}


	public void setNbNewNotifAugmentationMontantContrat(
			Integer nbNewNotifAugmentationMontantContrat) {
		this.nbNewNotifAugmentationMontantContrat = nbNewNotifAugmentationMontantContrat;
	}

	

	public Integer getNbAvisImprimer() {
		return nbAvisImprimer;
	}

	public void setNbAvisImprimer(Integer nbAvisImprimer) {
		this.nbAvisImprimer = nbAvisImprimer;
	}

	

	public Integer getNbRelance() {
		return nbRelance;
	}

	public void setNbRelance(Integer nbRelance) {
		this.nbRelance = nbRelance;
	}
	
}
